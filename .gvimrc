colorscheme onedark
set background=dark

set guifont=PragmataPro\ Mono:h15

set antialias
set lines=50 columns=160
" hide gui elements
set go=
" disable cursor blinking in normal mode
set guicursor=n:blinkon0

hi VertSplit guibg=NONE guifg=#839496
hi SignColumn guibg=NONE

let g:lightline = {
      \ 'colorscheme': 'default',
      \ }
